#%RAML 1.0
---
title: SnakePit
baseURi: http://ithiessen.deploy.cs.camosun.bc.ca
version: v1.0

/admin:
        get:
                description: Django Admin interface. Dont use this.
/games:
        /list:
                get:
                        description: Lists the games in the database. No parameters required for request.
                        responses:
                                200:
                                        body:
                                                application/json:
                                                        example: { "games": ["t_32", "a6d5b55a-2865-4e51-9452-8c7856e1de97", "3fdf4a73-4ef1-46ac-a8dd-ddea60ac26ba"]}
        /get:
                post:
                        description: Retrieves a specific game from the database. This will be a complete battlesnake game including all of the turns as JSON objects.

                        queryParameters:
                                gid:
                                        displayName: gid
                                        type: string
                                        description: Game ID
                                        example: t_32
                                        required: true
                        responses:
                                200:
                                        body:
                                                application/json:
                                                        example: {  "width": null,  "height": null,  "gid": "t_340",  "turn": [    {      "you": {        "health": 98,        "body": [          { "y": 3, "x": 14          },          { "y": 4, "x": 14          },          { "y": 4, "x": 13          }        ],        "name": "SekhnetSnek",        "id": "13e7d710-dfb9-45c2-919d-5dacbf609686"      },      "turn": 5,
