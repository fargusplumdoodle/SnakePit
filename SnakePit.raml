"board": {        "snakes": [          { "health": 98, "body": [ {   "y": 4,   "x": 11 }, {   "y": 3,   "x": 11 }, {   "y": 3,   "x": 12 } ], "name": "Son of Robosnake", "id":
"213cd80c-ef75-404c-968b-db2733ac6fbe"          },          { "health": 98, "body": [ {   "y": 3,   "x": 14 }, {   "y": 4,   "x": 14 }, {   "y": 4,   "x": 13 } ], "name": "SekhnetSnek", "id": "13e7d710-dfb9-45c2-919d-5dacbf609686"          }        ],        "width": 17,        "height": 10,        "food": [          { "y": 4, "x": 16          },
      { "y": 0, "x": 5          },          { "y": 7, "x": 8          },          { "y": 0, "x": 0          },          { "y": 0, "x": 7          },          { "y": 4, "x": 1
